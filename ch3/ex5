#include<iostream>

int main(){
	using namespace std;
	cout<<"Enter the world's population: ";
	long long int world_population;
	cin>> world_population;
	cout<<"Enter the population of the US: ";
	long long int US_population;
	cin>>US_population;
	long double kool=1.0*US_population/world_population*100;
	cout<<"The population of the US is "<<kool<<"% of the world population.";
	return 0;
 }
